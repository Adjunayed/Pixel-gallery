@Composable
fun WishlistScreen(navController: NavHostController) {
    Scaffold(topBar = { TopAppBar(title = { Text("Wishlist") }) }) {
        LazyColumn {
            items(5) { index ->
                ListItem(
                    headlineContent = { Text("Wishlist Item $index") },
                    supportingContent = { Text("Description...") }
                )
            }
        }
    }
}
